Host *
  ServerAliveInterval 30
  ServerAliveCountMax 3

Host 192.168.1.208
  HostName 192.168.1.208
  User ansible
  IdentityFile ~/.ssh/mediaserver_ansible_ed25519
  IdentitiesOnly yes

Host mediaserver-human
  HostName 192.168.1.208
  User joseph
  IdentityFile ~/.ssh/mediaserver_joseph_ed25519
  IdentitiesOnly yes

Host 10.3.0.3
  User ansible
  IdentityFile ~/.ssh/id_ed25519_ansible
  CertificateFile ~/.ssh/id_ed25519_ansible-cert.pub
  IdentitiesOnly yes
  BatchMode yes
  StrictHostKeyChecking accept-new


# BEGIN ANSIBLE MANAGED INCLUDE
Include ~/.ssh/config.d/*.conf

